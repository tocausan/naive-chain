{
  "name": "naive-chain",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./build/bin/www.js",
    "webpack": "webpack --config webpack.config.js && exit",
    "tsc": "rimraf ./build && tsc",
    "tsc:watch": "npm run tsc --watch",
    "dev": "concurrently --kill-others 'npm run tsc:watch' 'npm run webpack' 'nodemon'",
    "build": "npm install && bower install && npm run tsc && npm run webpack",
    "minify": "rimraf ./node_modules && rimraf ./build && rimraf ./src/public/components && rimraf ./src/public/dist"
  },
  "dependencies": {
    "bower": "^1.8.4",
    "cookie-parser": "^1.4.3",
    "crypto": "^1.0.1",
