    "html-loader": "^0.5.5",
    "jade": "^1.11.0",
    "mongoose": "^5.0.12",
    "nodemon": "^1.17.3",
    "npm-run-all": "^4.1.2",
    "qrcode": "^1.2.0",
    "rimraf": "^2.6.2",
    "socket.io-client": "^2.1.0",
    "style-loader": "^0.20.3",
    "vue": "^2.5.16",
    "vue-socket.io": "^2.1.1"
  },
  "devDependencies": {
    "@types/body-parser": "^1.16.8",
    "@types/chai": "^4.1.2",
