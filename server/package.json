    "@types/lodash": "^4.14.104",
    "@types/mocha": "^2.2.48",
    "@types/moment": "^2.13.0",
    "@types/mongodb": "^3.0.7",
    "@types/mongoose": "^5.0.7",
    "@types/morgan": "^1.7.35",
    "@types/node-sass-middleware": "0.0.30",
    "@types/serve-favicon": "^2.2.30",
    "@types/socket.io": "^1.4.33",
    "app-root-path": "^2.0.1",
    "clean-webpack-plugin": "^0.1.19",
    "concurrently": "^3.5.1",
